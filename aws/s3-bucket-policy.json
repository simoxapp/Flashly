{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowApplicationReadWrite",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::199537924101:user/FlasherUsers"
      },
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::flasherstorage",
        "arn:aws:s3:::flasherstorage/*"
      ]
    },
    {
      "Sid": "DenyInsecureTransport",
      "Effect": "Deny",
      "Principal": "*",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::flasherstorage",
        "arn:aws:s3:::flasherstorage/*"
      ],
      "Condition": {
        "Bool": {
          "aws:SecureTransport": "false"
        }
      }
    },
    {
      "Sid": "BlockPublicAccess",
      "Effect": "Deny",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::flasherstorage/*",
      "Condition": {
        "StringNotEquals": {
          "aws:PrincipalAccount": "199537924101"
        }
      }
    }
  ]
}